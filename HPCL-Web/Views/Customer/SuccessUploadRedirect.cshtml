@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>SuccessRedirect</title>
</head>
<body>
    <div class="bg-white p-2 p-md-4 w-100" style="background: #edecec !important;">
        <div class="bg-white p-3">
            <div>
                <ul class="breadcrumb bread_custom bg-transparent m-0 pb-1 py-0 px-0">
                    <li class="breadcrumb-item"><a href="@Url.Action("Index","Enrollments")" class="text_primary">Enrollments</a> </li>
                    <li class="breadcrumb-item font-weight-light">Upload Document</li>
                </ul>
            </div>
            <div class="createmessagearea alert alert-success">
                <p>KYC Documents Uploaded Successfully with Form Number: @ViewBag.FormNumber  &nbsp;&nbsp;<br /> @Html.ActionLink("Click Here to Create Cards", "AddCardDetails", new { FormNumber = @ViewBag.FormNumber }, null)</p>
            </div>
        </div>
    </div>
</body>
</html>