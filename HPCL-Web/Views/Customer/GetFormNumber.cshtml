
<div class="col-lg-12">
    <div class="bg-white p-2 p-md-4">
        <ul class="breadcrumb bread_custom bg-transparent m-0 pb-1 py-0 px-0">
            <li class="breadcrumb-item"><a href="@Url.Action("Index","Enrollments")" class="text_primary">Enrollments</a> </li>
            <li class="breadcrumb-item font-weight-light">Generate Form Number</li>
        </ul>
        <form>
            <div class="w-100">
                <div class="col-lg-12 col-md-12 col-12 pl-0 pr-0">
                    <div class="border-1 rounded border-grey pb-0">
                        <p class="text_primary px-3 py-2 bg-grey m-0 font-weight-bold">Generate Form Number</p>
                        <div class="col-lg-12 col-md-12 col-12 py-3">
                            <div class="d-flex align-items-center flex-wrap">
                                <div class="col-lg-12 col-12">
                                    <div class="d-flex align-items-center flex-wrap mb-0">
                                        <label class="font-weight-normal col-md-2 col-12">Form Number </label>
                                        <div class="col-sm-3 col-md-3 col-12">
                                            <div class="custom_select">
                                                <input type='text' id='txtFormNumber' class="form-control" readonly>
                                            </div>
                                        </div>
                                        <div class="col-md-7 col-12">
                                            <div class="d-flex align-items-center flex-wrap">
                                                <div class="px-0">
                                                    <button class="btn btn_primary regrate_btn" type="button" id="btnFormNumber" name="btnFormNumber">Generate Form Number</button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                        <div class="clearfix"></div>

                    </div>
                </div>
            </div>
        </form>
        <div class="clearfix"></div>
    </div>
</div>
<script type="text/javascript">
    
    $("#btnFormNumber").on('click', function (e) {


        $.ajax({
            url: "@Url.Action("GenerateFormNumber", "Customer")",
            type: "POST",
            dataType: "JSON",
            data: {},
            success: function (res) {
                console.log(res, "res");
                $('#txtFormNumber').val(res);
            },
            error: function (ex) {
                alert('Failed' + ex);
            }

        });

    });

</script>
