@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>SuccessRedirect</title>
</head>
<body>
    <div>
        <ul class="breadcrumb bread_custom bg-transparent m-0 pb-1 py-0 px-0">
            <li class="breadcrumb-item"><a href="@Url.Action("Index","Enrollments")" class="text_primary">Enrollments</a> </li>
            <li class="breadcrumb-item font-weight-light">Add New Customer</li>
        </ul>
    </div>
    <div class="createmessagearea alert alert-success">
        <p>@ViewBag.Message  &nbsp;&nbsp; <br />@Html.ActionLink("Upload KYC Documents", "UploadDoc", new { FormNumber = @ViewBag.FormNumber }, null)</p>
    </div>
</body>
</html>