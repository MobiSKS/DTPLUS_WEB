@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>SuccessRedirect</title>
</head>
<body>
    <div>
        <ul class="breadcrumb bread_custom bg-transparent m-0 pb-1 py-0 px-0">
            <li class="breadcrumb-item"><a href="@Url.Action("Index","Enrollments")" class="text_primary">Enrollments</a> </li>
            <li class="breadcrumb-item font-weight-light">Update KYC Document</li>
        </ul>
    </div>
    <div class="createmessagearea alert alert-success">
        <p>KYC Documents Updated Successfully with Form Number: @ViewBag.FormNumber  &nbsp;&nbsp;<br /> @Html.ActionLink("Validate New Customer", "ValidateNewCustomer", "Customer", null, null)</p>
    </div>
</body>
</html>