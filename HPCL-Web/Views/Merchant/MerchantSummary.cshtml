                    <div class="d-flex align-items-center justify-content-center mt-3">

                        <div class="px-2">
                            <button class="btn btn_primary px-4 py-2" id="btnCancel" type="submit">Cancel</button>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>

<script type="text/javascript">
    $('#btnCancel').click(function () {
        document.location = '@Url.Action("RejectedMerchant", "Merchant")';
    });
</script>
