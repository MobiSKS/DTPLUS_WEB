                                        <div class="col-md-7 col-12">
                                            <div class="row">
                                                <div class="col-4">
                                                    @*@Html.DropDownListFor(m => m.PreHighwayNumber, new SelectList(Model.PreHighwayNumbers, "PreHighwayNumberID", "PreHighwayNumberName"), new { @class = "form-control" })*@
                                                    <select name="comm_states" id="" class="form-control">
                                                        <option value="1">NH</option>
                                                        <option value="2">SH</option>
                                                    </select>
                                                </div>
                                                <div class="col-8 pl-0">
                                                    @Html.TextBoxFor(m => m.HighwayNumber, new { @class = "form-control" })
                                                </div>
                                            </div>
                                            <span class="error" id="highwayNum_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">Mapped Merchant Id<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.HighwayName, new { @class = "form-control" })
                                            <span class="error" id="highwayName_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12"></label>
                                        <div class="col-md-7 col-12">
                                            <div class="row">
                                                <div class="col-4">
                                                </div>
                                                <div class="col-8 pl-0">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">SBU Type<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.DropDownListFor(m => m.SBUType, new SelectList(Model.SBUTypes, "SBUId", "SBUName"), new { @class = "form-control" })
                                            <div class="error" id="sbuType_error"></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">
                                            LPG-CNG Sale<sup class="text-danger">*</sup><br />(3 months average in KL)
                                        </label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.LPG_CNGSale, new { @class = "form-control" })
                                            <div class="error" id="lpgCngSale_error"></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">Pan Card Number<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.PANCardNumber, new { @class = "form-control" })
