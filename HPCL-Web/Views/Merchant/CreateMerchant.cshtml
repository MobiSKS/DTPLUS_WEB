@model HPCL.Common.Models.ViewModel.Merchant.MerchantGetDetailsModel
@using Microsoft.AspNetCore.Http;
@using HPCL.Common.Resources

<div class="col-lg-12">
    <div class="bg-white p-2 p-md-4" style="background: #edecec !important;">
        <ul class="breadcrumb bread_custom bg-transparent m-0 pb-1 py-0 px-0">
            <li class="breadcrumb-item"><a href="@Url.Action("Profile", "Merchant")" class="text_primary">Profile</a> </li>
            <li class="breadcrumb-item font-weight-light">Manage Profile</li>
        </ul>
        <div class="w-100 bg-white p-3">
        <ul class="nav nav-tabs form_tabs" id="formTab" role="tablist">
            <li class="nav-item"> <a class="nav-link active" id="viewProfile-tab" data-toggle="tab" href="#viewProfile" role="tab" aria-controls="viewProfile" aria-selected="true">View Profile</a> </li>
            <li class="nav-item"> <a class="nav-link disable" id="basicInfo-tab" data-toggle="tab" href="#basicInfo" role="tab" aria-controls="basicInfo" aria-selected="false">Basic Information</a> </li>
            <li class="nav-item"> <a class="nav-link disable" id="address-tab" data-toggle="tab" href="#address" role="tab" aria-controls="address" aria-selected="false">Contact Details</a> </li>
            <li class="nav-item"> <a class="nav-link disable" id="terminalDetails-tab" data-toggle="tab" href="#terminalDetails" role="tab" aria-controls="terminalDetails" aria-selected="false">Terminal Details</a> </li>
            <li class="nav-item"> <a class="nav-link disable" id="summaryDetails-tab" data-toggle="tab" href="#summaryDetails" role="tab" aria-controls="summaryDetails" aria-selected="false">Summary Details</a> </li>
        </ul>
        @using (Html.BeginForm(null, null, FormMethod.Post, new { id = "applicationForm", name = "applicationForm" }))
        {
            <div class="tab-content mt-3" id="myTabContent">
                <div class="tab-pane fade show active" id="viewProfile" role="tabpanel" aria-labelledby="viewProfile-tab">
                    @if (Model.Message != "" && Model.Message != null)
                    {
                        <div class="alert alert-danger text-center">@Model.Message</div>
                    }
                    @if (Model.Success != "" && Model.Success != null)
                    {
                        <div class="alert alert-success text-center">@Model.Success</div>
                    }
                    <div class="border-1 rounded border-grey w-100 mt-3">
                        <p class="text-white px-3 py-2 m-0 font-weight-bold" style="background: #084693;">Search Profile</p>
                        <div class="py-2 p-md-4 bg-white">
                            <div class="d-flex align-items-center justify-content-between flex-wrap row">
                                <div class="col-lg-6 col-md-12 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-4 col-12">@Label.Merchant<sup class="text-danger">*</sup></label>
                                        <div class="col-md-8 col-12">
                                            <div class="row">
                                                @*@if (Context.Session.GetString("LoginType") == "MO" || Context.Session.GetString("LoginType") == "Admin")
                                                    {*@
                                                <div class="col-8">
                                                    @Html.TextBoxFor(m => m.SearchMerchantId, new { @class = "form-control",maxlength = "10", onkeypress = "return isNumberKey(event)" })
                                                </div>
                                                <div class="col-8" style="display:none">
                                                    @Html.TextBoxFor(m => m.Search, new { @class = "form-control" })
                                                </div>
                                                <div class="col-4">
                                                    <button class="btn btn_primary px-4 py-2" type="submit" onclick="checkForMerchantId(event)" style="width: 130%">SEARCH</button>
                                                </div>
                                                @*}
                                                    else
                                                    {
                                                    <div class="col-8">
                                                    @Html.TextBoxFor(m => m.MerchantId, new { @class = "form-control", @disabled = "disabled" })
                                                    </div>
                                                    @if (!string.IsNullOrEmpty(Context.Session.GetString("MerchantID")))
                                                    {
                                                    <div class="col-4">
                                                    <button class="btn btn_primary px-4 py-2" type="submit" onclick="" style="width: 130%">SEARCH</button>
                                                    </div>
                                                    }
                                                    }*@
                                            </div>
                                            <span class="error" id="searchMerchantId_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="clearfix"></div>
                                <div class="col-lg-6 col-md-12 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12"></label>
                                        <div class="col-md-7 col-12">
                                            <div class="row">
                                                <div class="col-8">
                                                </div>
                                                <div class="col-12">
                                                    @*@if (Context.Session.GetString("LoginType") == "MO" || Context.Session.GetString("LoginType") == "Admin")
                                                        {*@
                                                    <button class="btn btn_primary px-4 py-2" type="button" onclick="return showAddMerchant()" style="width: 60%;margin-left:45%;" id="addMerchantbtn">ADD MERCHANT</button>
                                                    @*}*@
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </div>
                <div class="tab-pane fade" id="basicInfo" role="tabpanel" aria-labelledby="basicInfo-tab">
                    <div class="border-1 rounded border-grey w-100 mt-3 w-100">
                        <p class="text-white px-3 py-2 m-0 font-weight-bold" style="background: #084693;">Basic Information</p>
                        <div class="py-2 p-md-4 bg-white">
                            <div class="d-flex align-items-center justify-content-between flex-wrap row">
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.Merchant</label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.MerchantId, new { @class = "form-control", @disabled = "disabled" })
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.RetailOutletName<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.RetailOutletName, new { @class = "form-control", maxlength = "40" })
                                            <span class="error" id="retailOutletName_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.MerchantType<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.DropDownListFor(m => m.MerchantTypeId, new SelectList(Model.MerchantTypes, "MerchantTypeCode", "MerchantTypeName"), new { @class = "form-control" })
                                            <span class="error" id="merchantType_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.DealerName<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.DealerName, new { @class = "form-control", maxlength = "50" })
                                            <span class="error" id="dealerName_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12 displayNone mappedMerchId">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.MappedMerchantId<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.MappedMerchantId, new { @class = "form-control" })
                                            <span class="error" id="mappedMerchantId_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.DealerMobileNo<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.DealerMobileNo, new { @class = "form-control", maxlength = "10", onkeypress = "return isNumberKey(event)", autocomplete = "off" })
                                            <span class="error" id="dealerMobileNum_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.OutletCategory<sup class="text-danger">*</sup></label>
