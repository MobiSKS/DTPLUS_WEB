    $("#ErpCode").on("blur", function () {
        if ($("#ErpCode").val().length > 0) {

            var erpCode = this.value;

            if (!erpCode.match(erpCodeCheck)) {
                document.getElementById("erpCode_error").innerHTML = "Start with 1 and Only 8 Digits are allowed";
            }
            else{
                $.ajax({
                    type: 'POST',  // http method
                    url: '@Url.Action("ValidateErpCode", "Common")',
                    data: { erpCode: erpCode },  // data to submit
                    dataType: "json",
                    success: function (data, status, xhr) {
                        var v = data;
                        if (status == 'success' && v == '0') {
                            document.getElementById("erpCode_error").innerHTML = "ERP Code already exists";
                        }
                        else {
                            document.getElementById("erpCode_error").innerHTML = "";
                            console.log(v);
                        }
                    }
                });
            }
        }
    });
