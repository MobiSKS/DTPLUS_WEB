                                        <div class="col-md-7 col-12">
                                            <div class="row">
                                                <div class="col-5">
                                                    @Html.DropDownListFor(m => m.HighwayNo1, new SelectList(Model.PreHighwayNumbers, "PreHighwayNumberID", "PreHighwayNumberName"), new { @class = "form-control" })
                                                </div>
                                                <div class="col-7 pl-0">
                                                    @Html.TextBoxFor(m => m.HighwayNo2, new { @class = "form-control", maxlength = "10", onkeypress = "return isNumberKey(event)" })
                                                </div>
                                            </div>
                                            <span class="error" id="highwayNum_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.HighwayName</label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.HighwayName, new { @class = "form-control", maxlength = "30" })
                                            <span class="error" id="highwayName_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12"></label>
                                        <div class="col-md-7 col-12">
                                            <div class="row">
                                                <div class="col-4">
                                                </div>
                                                <div class="col-8 pl-0">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    @if (Model.CreatedMerchant == "Y")
                                    {
                                        <div class="form-group d-flex align-items-center flex-wrap">
                                            <label class="font-weight-normal col-md-5 col-12">@Label.SBUType<sup class="text-danger">*</sup></label>
                                            <div class="col-md-7 col-12">
                                                @Html.DropDownListFor(m => m.SBUTypeId, new SelectList(Model.SBUTypes, "SBUId", "SBUName"), new { @class = "form-control" ,disabled="disabled"})
                                                @Html.HiddenFor(m => m.SBUTypeId)
                                                <div class="error" id="sbuType_error"></div>
                                            </div>
                                        </div>
                                    }
                                    else
                                    {
                                        <div class="form-group d-flex align-items-center flex-wrap">
                                            <label class="font-weight-normal col-md-5 col-12">@Label.SBUType<sup class="text-danger">*</sup></label>
                                            <div class="col-md-7 col-12">
                                                @Html.DropDownListFor(m => m.SBUTypeId, new SelectList(Model.SBUTypes, "SBUId", "SBUName"), new { @class = "form-control" })
                                                <div class="error" id="sbuType_error"></div>
                                            </div>
                                        </div>
                                    }
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">
                                            @Label.LpgCngSale<sup class="text-danger">*</sup><br />(3 months average in KL)
                                        </label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.LPGCNGSale, new { @class = "form-control", maxlength = "6", onkeypress = "return isNumberKey(event)", autocomplete = "off" })
                                            <div class="error" id="lpgCngSale_error"></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.PanCard<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.PancardNumber, new { @class = "form-control", @style = "text-transform:uppercase" })
                                            <div class="error" id="panCardNum_error"></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.GstNumber</label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.GSTNumber, new { @class = "form-control", @style = "text-transform:uppercase" })
                                            <div class="error" id="gstNum_error"></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="border-1 rounded border-grey w-100 mt-3 w-100">
                        <p class="text-white px-3 py-2 m-0 font-weight-bold" style="background: #084693;">Retail Outlet Address</p>
                        <div class="py-2 p-md-4 bg-white">
                            <div class="d-flex align-items-center justify-content-between flex-wrap row">
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.Address1<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.RetailOutletAddress1, new { @class = "form-control" })
                                            <span class="error" id="retail_address1_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.Address2<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.RetailOutletAddress2, new { @class = "form-control" })
                                            <span class="error" id="retail_address2_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.Address3</label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.RetailOutletAddress3, new { @class = "form-control" })
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.Location</label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.RetailOutletLocation, new { @class = "form-control", maxlength = "40" })
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.City<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.RetailOutletCity, new { @class = "form-control" })
                                            <span class="error" id="retail_city_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12"></label>
                                        <div class="col-md-7 col-12">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.State<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            <div class="custom_select">
                                                @Html.DropDownListFor(m => m.RetailOutletStateId, new SelectList(Model.RetailOutletStates, "StateID", "StateName"), new { @class = "form-control", onchange = "getRetailDistrict()" })
                                            </div>
                                            <span class="error" id="retail_state_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.District<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            <div class="custom_select">
                                                <select name="RetailOutletDistrictId" id="RetailOutletDistrictId" class="form-control"></select>
                                            </div>
                                            <span class="error" id="retail_district_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.Pin<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.RetailOutletPinNumber, new { @class = "form-control", maxlength = "6", onkeypress = "return isNumberKey(event)", autocomplete = "off" })
                                            <span class="error" id="retail_pincode_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.PhoneOfc<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.RetailOutletPhoneNumber, new { @class = "form-control", onkeypress = "return isNumberKey(event)" })
                                            <span class="error" id="retail_officePhone_error"></span>
                                        </div>
                                        @*<div class="col-md-7 col-12">
                                            <div class="row">
                                            <div class="col-4">
                                            @Html.TextBoxFor(m => m.Retail_Outlet_PhoneOfficeCode, new { @class = "form-control" })
                                            </div>
                                            <div class="col-8 pl-0">
                                            @Html.TextBoxFor(m => m.Retail_Outlet_PhoneOffice, new { @class = "form-control" })
                                            </div>
                                            <span class="error" id="retail_officePhone_error"></span>
                                            </div>
                                            </div>*@
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.Fax</label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.RetailOutletFax, new { @class = "form-control", onkeypress = "return isNumberKey(event)" })
                                            <span class="error" id="retail_faxNumber_error"></span>
                                        </div>
                                        @*<div class="col-md-7 col-12">
                                            <div class="row">
                                            <div class="col-4">
                                            @Html.TextBoxFor(m => m.Retail_Outlet_FaxCode, new { @class = "form-control" })
                                            </div>
                                            <div class="col-8 pl-0">
                                            @Html.TextBoxFor(m => m.Retail_Outlet_Fax, new { @class = "form-control" })
                                            </div>
                                            <span class="error" id="retail_faxNumber_error"></span>
                                            </div>
                                            </div>*@
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="border-1 rounded border-grey w-100 mt-3 w-100">
                        <p class="text-white px-3 py-2 m-0 font-weight-bold" style="background: #084693;">Business Details</p>
                        <div class="py-2 p-md-4 bg-white">
                            <div class="d-flex align-items-center justify-content-between flex-wrap row">
                                @if (Model.CreatedMerchant == "Y")
                                {  string  disabled = "disabled";
                                    <div class="col-md-6 col-12">
                                        <div class="form-group d-flex align-items-center flex-wrap">
                                            <label class="font-weight-normal col-md-5 col-12">@Label.ZonalOfc<sup class="text-danger">*</sup></label>
                                            <div class="col-md-7 col-12">
                                                @Html.DropDownListFor(m => m.ZonalOfficeId, new SelectList(Model.ZonalOffices, "ZonalOfficeID", "ZonalOfficeName"), new { @class = "form-control", onchange = "getMerchantRegionalOfc()",@disabled})
