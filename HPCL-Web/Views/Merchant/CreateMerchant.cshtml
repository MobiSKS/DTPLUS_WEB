        else if (!document.applicationForm.SearchMerchantId.value.match(mappedMerchantID)) {
            document.getElementById("searchMerchantId_error").innerHTML = "Invalid Merchant ID";
            if (ret != "N") {
                document.applicationForm.SearchMerchantId.focus();
            }
            $("#applicationForm").submit(function (e) {
                e.preventDefault();
            });
            ret = "N";
        }
        else {
            document.getElementById("searchMerchantId_error").innerHTML = "";
        }

        if (ret == "Y") {
            $("#Search").val("Search");
            $("#form1").unbind().submit();
