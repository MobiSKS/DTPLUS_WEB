                                            <div class="custom_select">
                                                @Html.DropDownListFor(m => m.OutletCategoryId, new SelectList(Model.OutletCategories, "OutletCategoryCode", "OutletCategoryName"), new { @class = "form-control" })
                                            </div>
                                            <span class="error" id="outletCategory_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12 highwayDetails">
                                    <div class="form-group d-flex align-items-center flex-wrap">
