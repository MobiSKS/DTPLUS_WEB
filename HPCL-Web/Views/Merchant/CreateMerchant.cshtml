@model HPCL.Common.Models.ViewModel.Merchant.MerchantGetDetailsModel
@using Microsoft.AspNetCore.Http;
@using HPCL.Common.Resources

<div class="col-lg-12">
    <div class="bg-white p-2 p-md-4">
        <ul class="breadcrumb bread_custom bg-transparent m-0 pb-1 py-0 px-0">
            <li class="breadcrumb-item"><a href="@Url.Action("Profile", "Merchant")" class="text_primary">Profile</a> </li>
            <li class="breadcrumb-item font-weight-light">Manage Profile</li>
        </ul>
        @*<h5>Manage Profile</h5>*@
        <ul class="nav nav-tabs mt-2 form_tabs" id="formTab" role="tablist">
            <li class="nav-item"> <a class="nav-link active" id="viewProfile-tab" data-toggle="tab" href="#viewProfile" role="tab" aria-controls="viewProfile" aria-selected="true">View Profile</a> </li>
            <li class="nav-item"> <a class="nav-link disable" id="basicInfo-tab" data-toggle="tab" href="#basicInfo" role="tab" aria-controls="basicInfo" aria-selected="false">Basic Information</a> </li>
            <li class="nav-item"> <a class="nav-link disable" id="address-tab" data-toggle="tab" href="#address" role="tab" aria-controls="address" aria-selected="false">Contact Details</a> </li>
            <li class="nav-item"> <a class="nav-link disable" id="terminalDetails-tab" data-toggle="tab" href="#terminalDetails" role="tab" aria-controls="terminalDetails" aria-selected="false">Terminal Details</a> </li>
            <li class="nav-item"> <a class="nav-link disable" id="summaryDetails-tab" data-toggle="tab" href="#summaryDetails" role="tab" aria-controls="summaryDetails" aria-selected="false">Summary Details</a> </li>
        </ul>
        @*<form name="applicationForm" method="post" action="@Url.Action("CreateMerchant", "Merchant")" class="w-100">*@
        @using (Html.BeginForm(null, null, FormMethod.Post, new { id = "applicationForm", name = "applicationForm" }))
        {
            <div class="tab-content mt-3" id="myTabContent">
                <div class="tab-pane fade show active" id="viewProfile" role="tabpanel" aria-labelledby="viewProfile-tab">
                    <div class="border-1 rounded border-grey w-100 mt-3">
                        @if (Model.Message != "")
                        {
                            <div class="mb-3 text-center" style="background-color: #f9c9c9; color: #8d0d0d; border-color: #f3abab;">@Model.Message</div>

                        }
                        @if (Model.Success != "")
                        {
                            <div class="mb-3 text-center" style="background-color: #a0f1d6;">@Model.Success</div>
                        }
                        <p class="text_primary px-3 py-2 bg-grey m-0 font-weight-bold">Search Profile</p>
                        <div class="py-2 p-md-4">
                            <div class="d-flex align-items-center justify-content-between flex-wrap row">
                                <div class="col-lg-6 col-md-12 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-4 col-12">@Label.Merchant<sup class="text-danger">*</sup></label>
                                        <div class="col-md-8 col-12">
                                            <div class="row">
                                                @*@if (Context.Session.GetString("LoginType") == "MO" || Context.Session.GetString("LoginType") == "Admin")
                                                    {*@
                                                <div class="col-8">
                                                    @Html.TextBoxFor(m => m.SearchMerchantId, new { @class = "form-control" })
                                                </div>
                                                <div class="col-8" style="display:none">
                                                    @Html.TextBoxFor(m => m.Search, new { @class = "form-control" })
                                                </div>
                                                <div class="col-4">
                                                    <button class="btn btn_primary px-4 py-2" type="submit" onclick="checkForMerchantId(event)" style="width: 130%">SEARCH</button>
                                                </div>
                                                @*}
                                                    else
                                                    {
                                                    <div class="col-8">
                                                    @Html.TextBoxFor(m => m.MerchantId, new { @class = "form-control", @disabled = "disabled" })
                                                    </div>
                                                    @if (!string.IsNullOrEmpty(Context.Session.GetString("MerchantID")))
                                                    {
                                                    <div class="col-4">
                                                    <button class="btn btn_primary px-4 py-2" type="submit" onclick="" style="width: 130%">SEARCH</button>
                                                    </div>
                                                    }
                                                    }*@
                                            </div>
                                            <span class="error" id="searchMerchantId_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="clearfix"></div>
                                <div class="col-lg-6 col-md-12 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12"></label>
                                        <div class="col-md-7 col-12">
                                            <div class="row">
                                                <div class="col-8">
                                                </div>
                                                <div class="col-12">
                                                    @*@if (Context.Session.GetString("LoginType") == "MO" || Context.Session.GetString("LoginType") == "Admin")
                                                        {*@
                                                    <button class="btn btn_primary px-4 py-2" type="button" onclick="return showAddMerchant()" style="width: 60%;margin-left:45%;" id="addMerchantbtn">ADD MERCHANT</button>
                                                    @*}*@
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </div>
                <div class="tab-pane fade" id="basicInfo" role="tabpanel" aria-labelledby="basicInfo-tab">
                    <div class="border-1 rounded border-grey w-100 mt-3 w-100">
                        <p class="text_primary px-3 py-2 bg-grey m-0 font-weight-bold">Basic Information</p>
                        <div class="py-2 p-md-4">
                            <div class="d-flex align-items-center justify-content-between flex-wrap row">
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.Merchant</label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.MerchantId, new { @class = "form-control", @disabled = "disabled" })
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.RetailOutletName<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.RetailOutletName, new { @class = "form-control", maxlength = "50" })
                                            <span class="error" id="retailOutletName_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.MerchantType<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.DropDownListFor(m => m.MerchantTypeId, new SelectList(Model.MerchantTypes, "MerchantTypeCode", "MerchantTypeName"), new { @class = "form-control, search_test" })
                                            <span class="error" id="merchantType_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.DealerName</label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.DealerName, new { @class = "form-control", maxlength = "50" })
                                            <span class="error" id="dealerName_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12 displayNone mappedMerchId">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.MappedMerchantId<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.MappedMerchantId, new { @class = "form-control" })
                                            <span class="error" id="mappedMerchantId_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.DealerMobileNo<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.DealerMobileNo, new { @class = "form-control", maxlength = "10", onkeypress = "return isNumberKey(event)", autocomplete = "off" })
                                            <span class="error" id="dealerMobileNum_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.OutletCategory<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            <div class="custom_select">
                                                @Html.DropDownListFor(m => m.OutletCategoryId, new SelectList(Model.OutletCategories, "OutletCategoryCode", "OutletCategoryName"), new { @class = "form-control" })
                                            </div>
                                            <span class="error" id="outletCategory_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12 highwayDetails">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12" id="highwayLabel">@Label.HighwayNo<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            <div class="row">
                                                <div class="col-4">
                                                    @Html.DropDownListFor(m => m.HighwayNo1, new SelectList(Model.PreHighwayNumbers, "PreHighwayNumberID", "PreHighwayNumberName"), new { @class = "form-control, search_test" })
                                                </div>
                                                <div class="col-8 pl-0">
                                                    @Html.TextBoxFor(m => m.HighwayNo2, new { @class = "form-control" })
                                                </div>
                                            </div>
                                            <span class="error" id="highwayNum_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.HighwayName</label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.HighwayName, new { @class = "form-control" })
                                            <span class="error" id="highwayName_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12"></label>
                                        <div class="col-md-7 col-12">
                                            <div class="row">
                                                <div class="col-4">
                                                </div>
                                                <div class="col-8 pl-0">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.SBUType<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.DropDownListFor(m => m.SBUTypeId, new SelectList(Model.SBUTypes, "SBUId", "SBUName"), new { @class = "form-control, search_test" })
                                            <div class="error" id="sbuType_error"></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">
                                            @Label.LpgCngSale<sup class="text-danger">*</sup><br />(3 months average in KL)
                                        </label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.LPGCNGSale, new { @class = "form-control", maxlength = "9", onkeypress = "return isNumberKey(event)", autocomplete = "off" })
                                            <div class="error" id="lpgCngSale_error"></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.PanCard<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.PancardNumber, new { @class = "form-control", @style = "text-transform:uppercase" })
                                            <div class="error" id="panCardNum_error"></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.GstNumber</label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.GSTNumber, new { @class = "form-control", @style = "text-transform:uppercase" })
                                            <div class="error" id="gstNum_error"></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="border-1 rounded border-grey w-100 mt-3 w-100">
                        <p class="text_primary px-3 py-2 bg-grey m-0 font-weight-bold">Retail Outlet Address</p>
                        <div class="py-2 p-md-4">
                            <div class="d-flex align-items-center justify-content-between flex-wrap row">
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.Address1<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.RetailOutletAddress1, new { @class = "form-control" })
                                            <span class="error" id="retail_address1_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.Address2<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.RetailOutletAddress2, new { @class = "form-control" })
                                            <span class="error" id="retail_address2_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.Address3</label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.RetailOutletAddress3, new { @class = "form-control" })
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.Location</label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.RetailOutletLocation, new { @class = "form-control" })
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.City<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.RetailOutletCity, new { @class = "form-control" })
                                            <span class="error" id="retail_city_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12"></label>
                                        <div class="col-md-7 col-12">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.State<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            <div class="custom_select">
                                                @Html.DropDownListFor(m => m.RetailOutletStateId, new SelectList(Model.RetailOutletStates, "StateID", "StateName"), new { @class = "form-control, search_test", onchange = "getRetailDistrict()" })
                                            </div>
                                            <span class="error" id="retail_state_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.District<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            <div class="custom_select">
                                                <select name="RetailOutletDistrictId" id="RetailOutletDistrictId" class="form-control"></select>
                                            </div>
                                            <span class="error" id="retail_district_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.Pin<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.RetailOutletPinNumber, new { @class = "form-control", maxlength = "6", onkeypress = "return isNumberKey(event)", autocomplete = "off" })
                                            <span class="error" id="retail_pincode_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.PhoneOfc<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.RetailOutletPhoneNumber, new { @class = "form-control" })
                                            <span class="error" id="retail_officePhone_error"></span>
                                        </div>
                                        @*<div class="col-md-7 col-12">
                                            <div class="row">
                                            <div class="col-4">
                                            @Html.TextBoxFor(m => m.Retail_Outlet_PhoneOfficeCode, new { @class = "form-control" })
                                            </div>
                                            <div class="col-8 pl-0">
                                            @Html.TextBoxFor(m => m.Retail_Outlet_PhoneOffice, new { @class = "form-control" })
                                            </div>
                                            <span class="error" id="retail_officePhone_error"></span>
                                            </div>
                                            </div>*@
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.Fax</label>
                                        <div class="col-md-7 col-12">
                                            @Html.TextBoxFor(m => m.RetailOutletFax, new { @class = "form-control" })
                                            <span class="error" id="retail_faxNumber_error"></span>
                                        </div>
                                        @*<div class="col-md-7 col-12">
                                            <div class="row">
                                            <div class="col-4">
                                            @Html.TextBoxFor(m => m.Retail_Outlet_FaxCode, new { @class = "form-control" })
                                            </div>
                                            <div class="col-8 pl-0">
                                            @Html.TextBoxFor(m => m.Retail_Outlet_Fax, new { @class = "form-control" })
                                            </div>
                                            <span class="error" id="retail_faxNumber_error"></span>
                                            </div>
                                            </div>*@
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="border-1 rounded border-grey w-100 mt-3 w-100">
                        <p class="text_primary px-3 py-2 bg-grey m-0 font-weight-bold">Business Details</p>
                        <div class="py-2 p-md-4">
                            <div class="d-flex align-items-center justify-content-between flex-wrap row">
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.ZonalOfc<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            @Html.DropDownListFor(m => m.ZonalOfficeId, new SelectList(Model.ZonalOffices, "ZonalOfficeID", "ZonalOfficeName"), new { @class = "form-control, search_test", onchange = "getMerchantRegionalOfc()" })
                                            <span class="error" id="zonal_office_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.RegionalOfc<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            <div class="custom_select">
                                                <select name="RegionalOfficeId" id="RegionalOfficeId" class="form-control" onchange="getMerchantSalesArea()"></select>
                                            </div>
                                            <span class="error" id="regionalOfc_error"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">@Label.SalesArea<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            <div class="custom_select">
                                                <select name="SalesAreaId" id="SalesAreaId" class="form-control"></select>
                                            </div>
                                            <span class="error" id="salesArea_error"></span>
                                        </div>
                                    </div>
                                </div>
                                @if (Model.CreatedMerchant == "Y")
                                {
                                    <div class="col-md-6 col-12">
                                        <div class="form-group d-flex align-items-center flex-wrap">
                                            <label class="font-weight-normal col-md-5 col-12">@Label.ErpCode<sup class="text-danger">*</sup></label>
                                            <div class="col-md-7 col-12">
                                                @Html.TextBoxFor(m => m.ErpCode, new { @class = "form-control", @disabled="disabled", @id="ErpCodeNoCheck" })
                                                <span class="error" id="erpCode_error"></span>
                                            </div>
                                        </div>
                                    </div>
                                }
                                else
                                {
                                    <div class="col-md-6 col-12">
                                        <div class="form-group d-flex align-items-center flex-wrap">
                                            <label class="font-weight-normal col-md-5 col-12">@Label.ErpCode<sup class="text-danger">*</sup></label>
                                            <div class="col-md-7 col-12">
                                                @Html.TextBoxFor(m => m.ErpCode, new { @class = "form-control" })
                                                <span class="error" id="erpCode_error"></span>
                                            </div>
                                        </div>
                                    </div>
                                }

                            </div>
                        </div>
                    </div>
                    <div class="border-1 rounded border-grey mt-3 w-100">
                        <p class="text_primary px-3 py-2 bg-grey m-0 font-weight-bold">Contact Person Details</p>
                        <div class="py-2 p-md-4">
                            <div class="d-flex align-items-center justify-content-between flex-wrap row">
