        @*<form name="applicationForm" method="post" action="@Url.Action("CreateMerchant", "Merchant")" class="w-100">*@
        @using (Html.BeginForm(null, null, FormMethod.Post, new { id = "applicationForm", name = "applicationForm" }))
        {
            <div class="tab-content mt-3" id="myTabContent">
                <div class="tab-pane fade show active" id="viewProfile" role="tabpanel" aria-labelledby="viewProfile-tab">
                    <div class="border-1 rounded border-grey w-100 mt-3">
                        <p class="text_primary px-3 py-2 bg-grey m-0 font-weight-bold">Search Profile</p>
                        <div class="py-2 p-md-4">
                            <div class="d-flex align-items-center justify-content-between flex-wrap row">
                                <div class="col-lg-6 col-md-12 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12">Merchant ID<sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12">
                                            <div class="row">
                                                @if (Context.Session.GetString("LoginType") == "MO" || Context.Session.GetString("LoginType") == "Admin")
                                                {
                                                    <div class="col-8">
                                                        @Html.TextBoxFor(m => m.SearchMerchantID, new { @class = "form-control" })
                                                    </div>
                                                    <div class="col-4">
                                                        <button class="btn btn_primary px-4 py-2" type="submit" onclick="" style="width: 130%">SEARCH</button>
                                                    </div>
                                                }
                                                else
                                                {
                                                    <div class="col-8">
                                                        @Html.TextBoxFor(m => m.SearchMerchantID, new { @class = "form-control", @disabled = "disabled" })
                                                    </div>
                                                    @if (!string.IsNullOrEmpty(Context.Session.GetString("MerchantID")))
                                                    {
                                                        <div class="col-4">
                                                            <button class="btn btn_primary px-4 py-2" type="submit" onclick="" style="width: 130%">SEARCH</button>
                                                        </div>
                                                    }
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-6 col-md-12 col-12">
                                    <div class="form-group d-flex align-items-center flex-wrap">
                                        <label class="font-weight-normal col-md-5 col-12"></label>
                                        <div class="col-md-7 col-12">
                                            <div class="row">
                                                <div class="col-8">
                                                </div>
                                                <div class="col-12">
                                                    @if (Context.Session.GetString("LoginType") == "MO" || Context.Session.GetString("LoginType") == "Admin")
                                                    {
                                                        <button class="btn btn_primary px-4 py-2" type="button" onclick="return showAddMerchant()" style="width: 60%;margin-left:45%;" id="addMerchantbtn">ADD MERCHANT</button>
                                                    }
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </div>
                <div class="tab-pane fade" id="basicInfo" role="tabpanel" aria-labelledby="basicInfo-tab">
