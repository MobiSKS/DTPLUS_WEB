                                            <div class="col-md-7 col-12">
                                                <div class="custom_select">
                                                    <select name="RegionalOfficeId" id="RegionalOfficeId" class="form-control" onchange="getMerchantSalesArea()"></select>
                                                </div>
                                                <span class="error" id="regionalOfc_error"></span>
                                            </div>
                                        </div>
