                </div>
            </div>
            <div class="clearfix"></div>
            <div class="d-flex align-items-center justify-content-center mt-0">
                <div class="px-2 mt-3">
                    <button class="btn btn_primary" type="submit" onclick="return ValidateMandatoryFields()">Save</button>
                </div>
            </div>

            <div style="display:none">
           <table id="OldVehicleDetails">
              <thead>
                 <tr>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                 </tr>
              </thead>
              <tbody>
                 @foreach (var item in Model.ObjJCBCardEntryDetail)
                 {
                 <tr>
                    <td>@item.CardNo</td>
                    <td>@item.VechileNo</td>
                    <td>@item.VehicleType</td>
                    <td>@item.VINNumber</td>
                    <td>@item.MobileNo</td>
                    <td>@item.VehicleNoMsg</td>
                    <td>@item.MobileNoMsg</td>
                    <td>@item.Verified</td>
