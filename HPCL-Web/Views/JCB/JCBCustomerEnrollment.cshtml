                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="d-flex align-items-center flex-wrap">
                                <div class="col-lg-6 col-12">
                                    <div class="d-flex align-items-center flex-wrap mb-3">
                                        <label class="font-weight-normal col-md-4 col-12">PAN Card <sup class="text-danger">*</sup></label>
                                        <div class="col-md-7 col-12 ">
                                            <div class="custom_select">
                                                @Html.TextBoxFor(m => m.PanCardProof, new { @id = "PanCardProof", @class = "form-control form-control-file myfilen", Type = "file" })
                                                <p class="mb-0">Only pdf,gif,png,jpeg,jpg,bmp,doc,docx file supported.<br />File size should not be greater than 1 MB.</p>
                                                @Html.ValidationMessageFor(m => m.PanCardProof, "", new { @id = "PanCardProofErr", @class = "text-danger" })
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-6 col-12">
                                    <div class="d-flex align-items-center flex-wrap mb-3">
                                        <label class="font-weight-normal col-md-5 col-12">Income Tax PAN </label>
                                        <div class="col-md-5 col-12">
                                            <div class="custom_select">
                                                @Html.TextBoxFor(m => m.PanCardNumber, new { @class = "form-control", @style = "text-transform:uppercase", autocomplete = "off", maxlength = "10" })
