                                            <span class='field-validation-error text-danger' id='MobileNo_error_@i'>
                                                @Model.ObjALCardEntryDetail[i].MobileNoMsg
                                            </span>
                                        </td>
                                        <td></td>
                                        <td style="display:none;">0</td>
                                        <td style="display:none;">
                                            @Model.ObjALCardEntryDetail[i].Verified                                       
                                         </td>
                                         <td style="display:none;">0</td>
                                    </tr>
                                }
                                else
                                {
                                    <tr>
                                        <td>@Slno</td>
                                       
                                            <td>
                                                <input type="text" maxlength="16" style="text-transform:uppercase" class="form-control" onkeypress="return isNumberKey(event)" name="ObjALCardEntryDetail[@i].CardNo" id="ObjALCardEntryDetail_@i" readonly>
