                    $("#" + dateId + "").html("");
                }
                
            }
            else{
                $("#" + dateId + "").attr("style", "display:block");
                $("#" + dateId + "").html("Invalid Date");
                dateflag = "N";
            }
        }

        return dateflag;
    }

