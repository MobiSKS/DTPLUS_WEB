    function ValidateVehicleNumber(vehicleNo)
    {
        var isValid = true;

        if(vehicleNo.trim()=='')
        {
            isValid=false;
        }
        else
        {
            if(vehicleNo.trim()!='')
            {
                if (!(vehicleNo.match(forVehicleNo)))
                {
                    isValid=false;
                }
                else if(vehicleNo.length<6 || vehicleNo.length>10)
                {
                    isValid=false;
                }
            }
        }

        return isValid;

