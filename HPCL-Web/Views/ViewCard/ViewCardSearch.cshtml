@model HPCL.Common.Models.ViewModel.ViewCard.ViewCardDetails

<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-ajax-unobtrusive/3.2.6/jquery.unobtrusive-ajax.js"></script>

<div class="bg-white p-2 p-md-4 w-100">

    @*<form name="applicationForm" onsubmit="return submitForm()" class="w-100">*@

    <form method="post" data-ajax="true" data-ajax-controller="ViewCard" data-ajax-action="ViewCardSearch" data-ajax-method="POST" data-ajax-success="OnSuccess" data-ajax-failure="OnFailure">

        <div class="border-1 rounded border-grey h-100 mt-3 mb-3">
            <p class="text_primary px-3 py-2 bg-grey m-0 font-weight-bold"> View Cards Limits </p>
            <div class="py-2 py-md-4">
                <div class="form-group d-flex align-items-center flex-wrap">
                    <div class="w-100">
                        <div class="d-flex align-items-center justify-content-between flex-wrap">
                            <div class="col-md-6 col-12">
                                <div class="form-group d-flex align-items-center flex-wrap">
                                    <label class="font-weight-normal col-md-5 col-12">CustomerID<sup class="text-danger">*</sup></label>
                                    <div class="col-md-7 col-12">
                                        @Html.TextBoxFor(m => m.Customerid, new { @class = "form-control", @Id = "Customerid" })
                                        @Html.ValidationMessageFor(m => m.Customerid, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-6 col-12">
                                <div class="form-group d-flex align-items-center flex-wrap">
                                    <label class="font-weight-normal col-md-5 col-12" id="Card" style="display:none">Card Number</label>
                                    <div class="col-md-7 col-12">
                                        @Html.TextBoxFor(m => m.CardNo, new { @class = "form-control", @Id = "CardNo", style = "display:none" })
                                        @Html.ValidationMessageFor(m => m.CardNo, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-12">
                                <div class="form-group d-flex align-items-center flex-wrap">
                                    <label class="font-weight-normal col-md-5 col-12" id="Mob" style="display:none">Mobile Number</label>
                                    <div class="col-md-7 col-12">
                                        @Html.TextBoxFor(m => m.MobileNo, new { @class = "form-control", @Id = "MobileNo", style = "display:none" })
                                        @Html.ValidationMessageFor(m => m.MobileNo, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-12">
                                <div class="form-group d-flex align-items-center flex-wrap">
                                    <label class="font-weight-normal col-md-5 col-12" id="Vec" style="display:none">Vechile Number</label>
                                    <div class="col-md-7 col-12">
                                        @Html.TextBoxFor(m => m.VechileNo, new { @class = "form-control", @Id = "VechileNo", style = "display:none" })
                                        @Html.ValidationMessageFor(m => m.VechileNo, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="d-flex align-items-center justify-content-center flex-wrap px-4 w-100 mb-0">
                        <div class="px-2 mb-3 col-sm-auto col-12 mb-sm-0 mb-2">
                            <button class="btn btn_primary" type="submit" id="btnSave" name="submit" onclick="showGrid()">SEARCH</button>
                        </div>
                        <div class="px-2 mb-3 col-sm-auto col-12 mb-sm-0 mb-2">
                            <a class="btn btn_primary" href="#" id="btnCancel" name="cancel" onclick="resetFields()">RESET</a>
                        </div>
                        <div class="px-2 mb-3 col-sm-auto col-12 mb-sm-0 mb-2">
                            <a class="btn btn_primary" href="#" id="btnrest" name="cancel" onclick="resetMapp()" style="display:none">RESET</a>
                        </div>
                        <div class="px-2 mb-3 col-sm-auto col-12 mb-sm-0 mb-2">
                            <a class="btn btn_primary" href="#" onclick="showTable()" id="btnAdd">ADD/EDIT MOBILE</a>
                        </div>
                        <div class="px-2 mb-3 col-sm-auto col-12 mb-sm-0 mb-2">
                            <a href="#" class="btn btn_primary" id="btnEdit" onClick="refresh" style="display:none">EDIT</a>
                        </div>
                        <div class="px-2 mb-3 col-sm-auto col-12 mb-sm-0 mb-2">
                            <button class="btn btn_primary" type="submit" id="btnADD" name="submit" style="display:none">ADD</button>
                        </div>
                        <div class="px-2 mb-3 col-sm-auto col-12 mb-sm-0 mb-2">
                            <a href="#" class="btn btn_primary" id="btnMapp" style="display:none">MAPPING DETAILS</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="mb-3 " id="CardSearch" style="display:none">
            <table class="table table-striped table-responsive  text-nowrap display" style="width:100%">
                <thead>
                    <tr>
                        <th>
                            @Html.DisplayName("Sr#")
                        </th>

                        <th>
                            @Html.DisplayName("Card Number")
                        </th>
                        <th>
                            @Html.DisplayName("Vehicle No/User Name")
                        </th>
                        <th>
                            @Html.DisplayName("Mobile Number")
                        </th>
                        <th>
                            @Html.DisplayName("Daily Sale Limit")
                        </th>
                        <th>
                            @Html.DisplayName("Daily Sale Balance")
                        </th>
                        <th>
                            @Html.DisplayName("Monthly Sale Limit")
                        </th>
                        <th>
                            @Html.DisplayName("Monthly Sale Balance")
                        </th>
                        <th>
                            @Html.DisplayName("CCMS Limit/Rs")
                        </th>
                        <th>
                            @Html.DisplayName("Type of Limit")
                        </th>
                        <th>
                            @Html.DisplayName("Availabe CCMS Limits")
                        </th>
                    </tr>
                </thead>
            </table>
        </div>

        <table id="CardEdit" class="table" style="display:none;">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayName(" ")
                    </th>

                    <th>
                        @Html.DisplayName("Card Number")
                    </th>
                    <th>
                        @Html.DisplayName("Vehicle No")
                    </th>
                    <th>
                        @Html.DisplayName("Mobile Number")
                    </th>
                    <th>
                        @Html.DisplayName("FastagNumber")
                    </th>
                    <th>
                        @Html.DisplayName("Action")
                    </th>

                </tr>
            </thead>
        </table>

        <div class="mb-3" id="Edit" style="display:none">
            <table class="table table-striped table-responsive d-md-table d-block text-nowrap display" style="width:100%">
                <thead>
                    <tr>
                        <th>
                            @Html.DisplayName(" ")
                        </th>

                        <th>
                            @Html.DisplayName("Card Number")
                        </th>
                        <th>
                            @Html.DisplayName("Vehicle No")
                        </th>
                        <th>
                            @Html.DisplayName("Mobile Number")
                        </th>
                        <th>
                            @Html.DisplayName("FastagNumber")
                        </th>
                        <th>
                            @Html.DisplayName("Action")
                        </th>
                    </tr>
                </thead>
            </table>
        </div>

        <table id="CardADD" class="table" style="display:none;">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayName(" ")
                    </th>

                    <th>
                        @Html.DisplayName("Card Number")
                    </th>
                    <th>
                        @Html.DisplayName("Vehicle No")
                    </th>
                    <th>
                        @Html.DisplayName("Mobile Number")
                    </th>
                    <th>
                        @Html.DisplayName("FastagNumber")
                    </th>
                </tr>
            </thead>
        </table>


        <table id="CardMapp" class="table" style="display:none;">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayName("Card Number")
                    </th>
                    <th>
                        @Html.DisplayName("Vehicle No")
                    </th>
                    <th>
                        @Html.DisplayName("Mobile Number")
                    </th>
                    <th>
                        @Html.DisplayName("FastagNumber")
                    </th>
                    <th>
                        @Html.DisplayName("Date")
                    </th>
                </tr>
            </thead>
        </table>

        <div class="form-group d-flex align-items-center justify-content-center flex-wrap mt-3">
            <div class="px-2">
                <button class="btn btn_primary" type="submit" id="save" name="submit" style="display:none;">Save</button>
                <button class="btn btn_primary" type="submit" id="Update" name="Update" style="display:none;">UPDATE</button>
            </div>
        </div>
    </form>
</div>
@using Microsoft.AspNetCore.Http;
<script type="text/javascript">
    function showGrid() {
        if ($("input:text").val() == "") {
            document.getElementById('CardSearch').style.display = 'none';
        }
        if (($("input:text").val() != "")) {
            document.getElementById('CardSearch').style.display = 'block';
        }
    }
    $('#Customerid').on("input", function (evt) {
        var self = $(this);
        self.val(self.val().replace(/\D/g, ""));
        if ((evt.which < 48 || evt.which > 57)) {
            evt.preventDefault();
        }
    });
    $('#Customerid').on("input", function (evt) {
        var cust = document.getElementById('Customerid').val();
        if (cust.length < 10 && cust.length > 20) {
            alert(false);
        }
    });

    function resetFields() {
        $("input:text").each(function () {
            $(this).val("");
        });
        //$("#CardSearch td").parent().remove();
        document.getElementById('CardSearch').style.display = 'none';
    }

    function OnSuccess(data) {


        console.log(data);

        $("#CardSearch td").parent().remove();

       // $("#CardSearch td").parent().remove();
        var rows = "<tbody>";
        for (var i = 0; i < data.searchList.length; i++) {

            rows += "<tr>"
                + "<td>" + (data.searchList[i].srNumber || '') + "</td>"
                + "<td>" + (data.searchList[i].cardNumber || '') + "</td>"
                + "<td>" + (data.searchList[i].vechileNo || '') + "</td>"
                + "<td>" + (data.searchList[i].mobileNumber || '') + "</td>"
                + "<td>" + (data.searchList[i].dailySaleLimit || '') + "</td>"
                + "<td>" + (data.searchList[i].dailySaleBal || '') + "</td>"
                + "<td>" + (data.searchList[i].monthlySaleLimit || '') + "</td>"
                + "<td>" + (data.searchList[i].monthlySaleBal  || '') + "</td>"
                + "<td>" + (data.searchList[i].cCMSLimit|| '') + "</td>"
                + "<td>" + (data.searchList[i].limitType || '') + "</td>"
                + "<td>" + (data.searchList[i].availableCCMSLimit || '') + "</td>"

                + "</tr >";


        }
          rows += "</tbody >";
           $('#CardSearch table').append(rows);

           $('#CardSearch table').DataTable({
				"lengthMenu": [5, 10, 20, 50, 100, 200, 500, 1000, 2000],
                "autoWidth": false,
				"oLanguage": {
					"sLengthMenu": "Display _MENU_ Records",
					"info": "Showing _START_ to _END_ of _TOTAL_ Records",
				}
			});


        let tableData = $('#CardSearch table').html();
        sessionStorage.setItem("tableData", tableData);

        let custmId = $('.cusId').html();
        sessionStorage.setItem("cusmId", custmId);
    }



    function OnFailure(data) {
        alert('HTTP Status Code: ' + data + '  Error Message: ' + data);
    }

    $("#CardSearch").on('click', '.updateStatus', function (e) {

        var currentRow = $(this).closest("tr");

        var cardNumber = currentRow.find(".crdNo").text();


        console.log(customerid, "customerID");



        $.ajax({
            url: "@Url.Action("UpdateStatus","ViewCard")",
            type: "POST",
